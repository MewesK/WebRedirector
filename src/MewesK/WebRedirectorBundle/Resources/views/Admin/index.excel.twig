{% xlsdocument %}
    {% xlssheet 'Redirects' %}
        {% xlsrow %}
            {% xlscell %}#{% endxlscell %}
            {% xlscell %}Position{% endxlscell %}
            {% xlscell %}Hostname{% endxlscell %}
            {% xlscell %}Path{% endxlscell %}
            {% xlscell %}Destination{% endxlscell %}
            {% xlscell %}Use Regex{% endxlscell %}
            {% xlscell %}Use Placeholders{% endxlscell %}
        {% endxlsrow %}
        {% for entity in entities %}
            {% xlsrow %}
                {% xlscell %}{{ entity.id }}{% endxlscell %}
                {% xlscell %}{{ entity.position }}{% endxlscell %}
                {% xlscell %}{{ entity.hostname }}{% endxlscell %}
                {% xlscell %}{{ entity.path }}{% endxlscell %}
                {% xlscell %}{{ entity.destination }}{% endxlscell %}
                {% xlscell %}{{ entity.useRegex }}{% endxlscell %}
                {% xlscell %}{{ entity.usePlaceholders }}{% endxlscell %}
            {% endxlsrow %}
        {% endfor %}
    {% endxlssheet %}
{% endxlsdocument %}